<p style="color: green"><%= notice %></p>

<h1>Physicians</h1>

<%= link_to "New physician", new_physician_path %>
<div id="physicians">

  <table class="table">
      <thead class="thead-dark">
        <tr>
          <th scope="col">#</th>
          <th scope="col">physician Name</th>
          <th scope="col">Email</th>
          <th scope="col">Unique Id</th>
          <th scope="col">Total Apointments</th>
          <th scope="col">Action</th>
          
        </tr>
      </thead>
      <tbody>
      <% @physicians.each do |physician| %>
        <%= render "physician", physician: physician, show: "true"%>
      <% end%>
      </tbody>
  </table>
</div>